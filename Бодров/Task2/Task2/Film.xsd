<?xml version="1.0" encoding="utf-8"?>
<xs:schema id="Film"
    targetNamespace="http://tempuri.org/Scheme.xsd"
    elementFormDefault="qualified"
    xmlns="http://tempuri.org/Scheme.xsd"
    xmlns:mstns="http://tempuri.org/Scheme.xsd"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
>
  <xs:element name="FilmLibrary">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="FILM" maxOccurs="unbounded">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="PRODUCER" type="xs:string"/>
              <xs:element name="NAME" type="xs:string"/>
              <xs:element name="GENRE" type="xs:string"/>
              <xs:element name="PREMIERE" type="xs:int"/>
              <xs:element name="BUDGET" type="xs:float"/>
            </xs:sequence>
            <xs:attribute name="Title" type="xs:string"/>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
</xs:schema>